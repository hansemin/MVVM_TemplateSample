<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:contents="clr-namespace:ViewModels.Contents;assembly=ViewModels"
                    xmlns:frames="clr-namespace:Views.Frames">
	
	<DataTemplate DataType="{x:Type contents:MainViewModel}">
		<frames:MainView/>
	</DataTemplate>

	<DataTemplate DataType="{x:Type contents:NextViewModel}">
		<frames:NextView/>
	</DataTemplate>

</ResourceDictionary>